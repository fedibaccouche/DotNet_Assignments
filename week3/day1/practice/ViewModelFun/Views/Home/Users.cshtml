@{
    ViewData["Title"] = "Home Page";
}
@model List<User>
<div class="text-center">
    <div class="container">
        <div class="nav-bar">

            <a class="link" href="/"> Home</a>
            <div>|</div>
            <a class="link" href="/numbers"> Numbers</a>
            <div>|</div>
            <a class="link" href="/user"> User</a>
            <div>|</div>
            <p> Users</p>

        </div>  
        <h1>
            Here is a list of users
        </h1>
        <div class="card" >
            
            <div class="card-body">
                @foreach (User  u in Model)
                {
                    <h5 class="card-title">@u.FirstName @u.LastName</h5>
                }
                
           
            </div>
        </div>

    </div>
</div>